package question9;

import java.util.*;

public class MonitoredList {
	 HashMap<String, Equipment> monitoredList;
	
	public MonitoredList() {
		this.monitoredList = new HashMap<>();
	}
	
	public void addEquipment(Equipment e) {
		this.monitoredList.put(e.getIP(), e);
	}
	
	public static void main(String[] args) {
		MonitoredList m = new MonitoredList();
		
		Robots b = new Robots("D");
		Robots c = new Robots("D");
		
		m.addEquipment(c);
		m.addEquipment(b);
		
		System.out.println(m.class instanceof Object);
	}
}
