package assignment1;

import java.util.Scanner;
import java.util.Stack;

import assignment1.Exceptions.InvalidMoveException;

public class TESTStack {

    static Stack<Integer> stack = new Stack<Integer>();

    public static void main(String[] args) {
    	int row, col;
    	Move newMove;
    	
    	Scanner scanner = new Scanner(System.in);
		System.out.print("Please input your fromCell (e.g. colraw, E2): ");
		String fromCell = scanner.next();
//		System.out.print("Please input your toCell (e.g. colraw, E2): ");
//		String toCell = scanner.next();
//		System.out.println(fromCell + " -> " + toCell);
		try {
			row = Utils.convertCharToInt(fromCell.charAt(0));
			col = Utils.convertCharToInt(fromCell.charAt(1));
//			newMove = new Move()
			System.out.println(row + col);
		} catch (InvalidMoveException e) {
			System.out.println("Not an Invalid Move");
		}
		scanner.close();
    }
}
