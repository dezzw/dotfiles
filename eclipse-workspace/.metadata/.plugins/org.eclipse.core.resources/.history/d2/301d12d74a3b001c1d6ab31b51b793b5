package question8;

public class ExceptionQuestion {
	public int helper(int n) throws IncalidArgumentException {
		if (n < 0) throw new IncalidArgumentException();
		return n;
	}
	
	public boolean doSomething(int n) {
		try {
			return helper(n) > 0;
		} catch (IncalidArgumentException e) {
			return false;
		}
	}
	
	public static void main(String[] args) {
		ExceptionQuestion e = new ExceptionQuestion();
		System.out.println(e.doSomething(-1));
	}
}
