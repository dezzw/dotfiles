package assignment1;

public class Observable {
	
	public int size = 5;
	public final Observer[][] board = new Observer[size][size];
	
	public void add(Cell cell, int row, int col) {
		this.board[row][col] = cell;
	}
	
	public void notifyObservers(Cell cell, Piece piece) {
		board[cell.getCoordinate().row][cell.getCoordinate().col].update(piece);
	}

}
